#include <stdio.h>
int main()
{
    int i,a,b,c,ans=0;
    printf ("Enter three numbers: ");
    scanf ("%d %d %d",&a,&b,&c);
    for (i=a; i>0; i--)
    {
        if (a%i==0 && b%i==0 && c%i==0)
        {
            ans=i;
            break;
        }
    }
    printf ("The GCD of %d, %d and %d is: %d",a,b,c,ans);
    return 0;
}
