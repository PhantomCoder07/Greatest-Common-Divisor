#include <stdio.h>
int main()
{
    int i,a,b,ans=0;
    printf ("Enter two numbers: ");
    scanf ("%d %d",&a,&b);
    i=a;
    do
    {
        if (a%i==0 && b%i==0)
        {
            ans=i;
            break;
        }
        i--;
    }
    while (i>0);
    printf ("The GCD of %d and %d is= %d",a,b,ans);
    return 0;
}
